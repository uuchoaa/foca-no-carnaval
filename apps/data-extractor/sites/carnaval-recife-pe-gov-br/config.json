{
  "name": "carnaval-recife-pe-gov-br",
  "url": "https://carnaval.recife.pe.gov.br/",
  "description": "Data extraction for carnaval-recife-pe-gov-br",
  "snapshot": {
    "sanitize": {
      "removeScripts": true,
      "removeStyles": true,
      "removeComments": true,
      "removeSVG": true,
      "removeIframes": true,
      "removeHidden": true,
      "additionalSelectors": []
    }
  },
  "pipeline": [
    {
      "type": "reader",
      "module": "readers/extract-list.js"
    },
    {
      "type": "parallel",
      "concurrency": 10,
      "module": "readers/extract-details.js",
      "input": "output.json",
      "itemsPath": "items",
      "urlField": "url",
      "output": "output-with-details.json"
    }
  ],
  "metadata": {}
}